{
  "parser": "babel-eslint",
  "extends": ["airbnb"],
  "rules": {
    "max-len": [1, 70, 2, { "ignoreComments": true }],
    "no-unused-vars": 0,
    "no-use-before-define": ["error", { "functions": false }],
    "operator-linebreak": [
      2,
      "after",
      { "overrides": { "?": "none" } }
    ],
    "import/prefer-default-export": "off"
  },
  "env": {
    "es6": true,
    "browser": true
  }
}
